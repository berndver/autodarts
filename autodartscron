# /etc/cron.d/autodartscron: crontab entries for the autodarts system

# Umgebungsvariablen
PATH=/usr/local/sbin:/usr/local/bin:/usr/sbin:/usr/bin:/sbin:/bin
SHELL=/bin/bash

@reboot root /bin/bash /autodarts/auto-update.sh >> /autodarts/autodartscron.log 2>&1
@reboot root echo running-autodarts-crontab >> /autodarts/autodartscron.log 2>&1
@midnight root /bin/bash /autodarts/auto-update.sh >> /autodarts/autodartscron.log 2>&1

