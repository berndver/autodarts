# /etc/cron.d/autodartscron: crontab entries for the autodarts system

# Umgebungsvariablen
PATH=/usr/local/sbin:/usr/local/bin:/usr/sbin:/usr/bin:/sbin:/bin
SHELL=/bin/bash

@reboot root /bin/bash /autodarts/auto-update.sh > /autodarts/autodartscron.log
@reboot root echo running-autodarts-crontab > /autodarts/autodartscron.log
@midnight root /bin/bash /autodarts/auto-update.sh > /autodarts/autodartscron.log

